{
  "name": "react-ws-kit",
  "version": "1.0.1",
  "description": "Production-quality typed WebSocket hook for React with intelligent connection sharing, auto-reconnect, and message queuing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sameerdewan/react-ws-kit.git"
  },
  "bugs": {
    "url": "https://github.com/sameerdewan/react-ws-kit/issues"
  },
  "homepage": "https://github.com/sameerdewan/react-ws-kit#readme",
  "author": "Sameer Dewan <dewansameer@protonmail.com>",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.45",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.1.0",
    "happy-dom": "^12.10.3",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "react",
    "websocket",
    "hook",
    "typescript",
    "realtime",
    "ws",
    "connection-sharing",
    "auto-reconnect",
    "message-queue",
    "typed-websocket",
    "react-hook",
    "websocket-client"
  ],
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  }
}
